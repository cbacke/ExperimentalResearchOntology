@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .

@prefix ero-alias: <http://w3id.org/ExperimentalResearchOntology/alias#> .
@prefix ero-core: <http://w3id.org/ExperimentalResearchOntology/core#> .
@prefix rfp: <http://w3id.org/RoBivaL/FDOProfile/> .

ero-core:ExperimentParameter
    a owl:Class ;
    dcterms:conformsTo rfp:TrivialProfile ;
    rdfs:label "Experiment Parameter" ;
    rdfs:comment "A measurable or observable property that characterizes a specific type of robot experiment." ;
    rdfs:subClassOf
        ero-alias:BFO_quality ,
        rdf:Property ,
        sosa:ObservableProperty ,
    [
        a owl:Restriction ;
        owl:onProperty rdfs:domain ;
        owl:hasValue ero-core:ExperimentRun ;
    ] ;
.
